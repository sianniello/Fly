<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from FlyPortalDataService WSDL service -->

  <service name="FlyPortalData" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://webServices.com"/>
      <parameter name="wsdlServiceElement" value="FlyPortalDataService"/>
      <parameter name="schemaQualified" value="http://webServices.com"/>
      <parameter name="wsdlServicePort" value="FlyPortalData"/>
      <parameter name="className" value="com.webServices.FlyPortalDataSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="FlyPortalData"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getAirlines" qname="operNS:getAirlines" xmlns:operNS="http://webServices.com" returnQName="retNS:getAirlinesReturn" xmlns:retNS="http://webServices.com" returnType="rtns:ArrayOf_xsd_anyType" xmlns:rtns="http://webServices.com" returnItemQName="tns:item" xmlns:tns="http://webServices.com" soapAction="" >
      </operation>
      <operation name="getAirports" qname="operNS:getAirports" xmlns:operNS="http://webServices.com" returnQName="retNS:getAirportsReturn" xmlns:retNS="http://webServices.com" returnType="rtns:ArrayOf_xsd_anyType" xmlns:rtns="http://webServices.com" returnItemQName="tns:item" xmlns:tns="http://webServices.com" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="getAirlines getAirports"/>

      <typeMapping
        xmlns:ns="http://webServices.com"
        qname="ns:>getAirportsResponse"
        type="java:com.webServices.GetAirportsResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <arrayMapping
        xmlns:ns="http://webServices.com"
        qname="ns:ArrayOf_xsd_anyType"
        type="java:java.lang.Object[]"
        innerType="cmp-ns:anyType" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://webServices.com"
        qname="ns:>getAirlinesResponse"
        type="java:com.webServices.GetAirlinesResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://webServices.com"
        qname="ns:>getAirlines"
        type="java:com.webServices.GetAirlines"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://webServices.com"
        qname="ns:>getAirports"
        type="java:com.webServices.GetAirports"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
